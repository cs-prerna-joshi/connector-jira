{
  "name": "jira",
  "label": "Jira",
  "description": "Jira Service Desk Connector for issue creating, updating and deleting",
  "publisher": "Fortinet",
  "cs_approved": true,
  "cs_compatible": true,
  "version": "1.1.0",
  "category": "Ticket Creation",
  "icon_small_name": "jira_small.png",
  "icon_large_name": "jira_large.png",
  "help_online": "https://help.cybersponse.com/support/solutions/articles/17000101338-jira-v1-1-0",
  "configuration": {
    "fields": [
      {
        "title": "Server URL",
        "description": "URL for the Jira server from where the connector gets notifications.",
        "required": true,
        "editable": true,
        "visible": true,
        "type": "text",
        "name": "server_url"
      },
      {
        "title": "Username",
        "description": "Username configured for your account to access the Jira server from where the connector gets notifications.",
        "required": true,
        "editable": true,
        "visible": true,
        "type": "text",
        "name": "username"
      },
      {
        "title": "API Token",
        "description": "API Token configured for your account to access the Jira server from where the connector gets notifications.",
        "required": true,
        "editable": true,
        "visible": true,
        "type": "password",
        "name": "token"
      },
      {
        "title": "Verify SSL",
        "description": "Specifies whether the SSL certificate for the server is to be verified or not.Defaults to True.",
        "required": false,
        "editable": true,
        "visible": true,
        "type": "checkbox",
        "name": "verify_ssl",
        "value": true
      }
    ]
  },
  "operations": [
    {
      "operation": "create_ticket",
      "title": "Create Ticket",
      "description": "Creates a Jira ticket on your configured Jira server based on the project key, ticket summary, type, and description, and other input parameters you have specified.",
      "category": "investigation",
      "annotation": "create_ticket",
      "output_schema": {
        "id": "",
        "key": "",
        "self": "",
        "status": ""
      },
      "enabled": true,
      "parameters": [
        {
          "title": "Project Key",
          "description": "Unique project ID that represents the project under which you want to create the Jira ticket.",
          "required": true,
          "editable": true,
          "visible": true,
          "type": "text",
          "name": "project_key",
          "value": "",
          "placeholder": "e.g. JIR"
        },
        {
          "title": "Ticket Summary",
          "description": "Summary or heading of the ticket that you want to create on your configured Jira server.",
          "required": true,
          "editable": true,
          "visible": true,
          "type": "text",
          "name": "ticket_summary",
          "value": ""
        },
        {
          "title": "Ticket Description",
          "description": "Description of the ticket that you want to create on your configured Jira server.",
          "required": true,
          "editable": true,
          "visible": true,
          "type": "text",
          "name": "ticket_description",
          "value": ""
        },
        {
          "title": "Ticket Type",
          "description": "Type of ticket that you want to create on your configured Jira server.You can add one of the following ticket types: IT Help, Service Request, Service Request with Approvals, Task, or Sub-task.",
          "required": true,
          "editable": true,
          "visible": true,
          "type": "text",
          "name": "issue_type",
          "tooltip": "IT Help, Service Request, Service Request with Approvals, Task, Sub-task",
          "value": ""
        },
        {
          "title": "Priority",
          "description": "(Optional) Priority that you want to set for the Jira ticket that you want to update on your configured Jira server.You can add one of the following priorities: Highest, High, Medium, Low, or Lowest.",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "text",
          "name": "priority",
          "tooltip": "Highest, High, Medium, Low, Lowest",
          "value": ""
        },
        {
          "title": "Other Fields",
          "description": "(Optional) Fields such as Reporter, Assignee, Components that you need to set while creating a ticket.For example: {\"reporter\": {\"name\":\"admin\"}, \"assignee\":{\"name\":\"admin\"}, \"components\":[{\"name\":\"Intranet\"}]}",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "text",
          "name": "other_fields",
          "tooltip": "Other fields can be Reporter, Assignee, Components etc",
          "placeholder": "e.g {\"assignee\": {\"accountId\": \"5ac21efbb31cf552a3acb08c\"}}"
        }
      ]
    },
